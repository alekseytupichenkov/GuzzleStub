{
    "name": "alekseytupichenkov/guzzle_stub_bundle",
    "description": "This bundles provides guzzle stubs",
    "keywords": ["symfony2"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tupichenkov Aleksey",
            "homepage": "https://tupichenkov.com/"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "symfony/symfony": "^2.7.1|^3.3|^4.0",
        "symfony/phpunit-bridge": "^2.7|^3.0|^4.0",
        "phpunit/phpunit": "^4.8.35|^5.7|^6.1"
    },
    "autoload": {
        "psr-4": {
            "Alekseytupichenkov\\GuzzleStubBundle\\": "src",
            "Alekseytupichenkov\\GuzzleStubBundle\\Tests\\": "Tests"
        }
    }
}
